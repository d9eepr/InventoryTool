Login 



	list of users 
		fullname     varchar(200)
		empid         varchar2(8)
		email 			varchar2(20)
		password		varchar2(20)
		date of joining	date
		type			varchar(10)
		
		
		
Create table user (
	empid varchar(20) PRIMARY KEY ,
	fullname varchar(200),
	email varchar(200),
	upassword varchar(20),
	dateOfJoining date,
	utype varchar(10)
);
	


